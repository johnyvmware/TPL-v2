{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DataFlow": {
    "Engine": "TPL",
    "Configuration": {
      "MaxConcurrency": 4,
      "ProcessingTimeout": "00:00:30",
      "EnableBackpressure": true,
      "BufferSize": 1000
    },
    "Processors": [
      {
        "Id": "validator",
        "Type": "ValidationProcessor",
        "Enabled": true
      },
      {
        "Id": "transformer",
        "Type": "TransformProcessor",
        "Enabled": true
      },
      {
        "Id": "logger",
        "Type": "LoggingProcessor",
        "Enabled": true
      }
    ]
  }
}