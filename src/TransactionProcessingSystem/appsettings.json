{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "Llm": {
    "OpenAI": {
      "MaxTokens": 4000,
      "Temperature": 1
    },
    "Prompts": {
      "Path": "llm\\prompts",
      "CategorizerDeveloperMessage": "categorizer-developer-message.txt",
      "CategorizerUserMessage": "categorizer-user-message.txt"
    },
    "StructuredOutputs": {
      "Path": "llm\\outputs",
      "Categorizer": "categorizer-output.json"
    }
  },
  "Categories": {
    "Path": "data\\categories\\categories.json"
  },
  "MicrosoftGraph": {
    "EmailSearchDays": 3
  },
  "Export": {
    "FileNameFormat": "transactions_{0}.json",
    "BufferSize": 8192
  },
  "Pipeline": {
    "InputBufferCapacity": 80,
    "TimeoutMinutes": 2
  },
  "Neo4j": {
    "MaxConnectionPoolSize": 10,
    "ConnectionTimeoutSeconds": 15,
    "MaxTransactionRetryTimeSeconds": 15
  },
  "TransactionFetcher": {
    "InputDirectory": "data\\transactions",
    "Encoding": "windows-1250"
  }
}